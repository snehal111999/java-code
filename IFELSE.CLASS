package PrecourseBeta;

public class ifelse {
	public static void main(String args[]) {
		 int num1 = 200;
		 int num2 = 300;
		 
		 if(num1 == num2) { //200, 100
			 System.out.println("Equal");
		 }else {
			 System.out.println("Not Equal");
		 }
		 
		 
		 System.out.println(100 == 200);// false
		 System.out.println(1000 == 1000); // true
		 System.out.println(100 > 20); // true
		 System.out.println(10 > 1000); //false
		 
		 System.out.println(100 < 1000);//true
		 System.out.println(22000 < 1000);//false
		 
		 //greater than and equal to
		 System.out.println(1000 > 1000); // false
		 System.out.println(1000 >= 1000); // true
		 System.out.println(100 <= 1000); // true
		 System.out.println(100 <= 100); // true
		 
		 System.out.println(200 <= 100); // false
		 
		 
		 System.out.println(100 == 200);//false
		 System.out.println(100 != 200); //true
		 System.out.println(100 != 100); //false
		 
		 
		 int a = 50;
		 int b = 30;
		 if(a>b) {
			 System.out.println(a);
		 }else {
			 System.out.println(b); // 30
		 }
		 
		 
		 
		 
	}
}
